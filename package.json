{
  "name": "electron-study",
  "version": "1.0.0",
  "description": "",
  "main": "./output/main/index.js",
  "scripts": {
    "start": "electron .",
    "dev-main": "cross-env NODE_ENV=development webpack --watch --config ./config/main.webpack.config.js",
    "dev-render": "cross-env NODE_ENV=development webpack server --config ./config/render.webpack.config.js",
    "dev": "concurrently -n main,renderer \"npm run dev-main\" \"npm run dev-render\"",
    "build-main": "cross-env NODE_ENV=production webpack --config ./config/main.webpack.config.js",
    "build-render": "cross-env NODE_ENV=production webpack --config ./config/render.webpack.config.js",
    "build": "concurrently -n main,renderer \"npm run build-main\" \"npm run build-render\""
  },
  "dependencies": {
    "antd": "^5.10.0",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.192",
    "@types/react": "^18.2.22",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "css-loader": "^6.7.3",
    "style-loader": "^3.3.2",
    "clean-webpack-plugin": "^4.0.0",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "electron": "^27.0.2",
    "eslint": "^7.32.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-webpack-plugin": "^2.7.0",
    "html-webpack-plugin": "^5.5.3",
    "ts-loader": "^9.5.0",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "keywords": [],
  "author": "alun",
  "license": "ISC"
}
